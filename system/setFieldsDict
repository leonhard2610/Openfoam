FoamFile
{
    format      ascii;
    class       dictionary;
    object      setFieldsDict;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

defaultFieldValues
(
    volScalarFieldValue T 300
    volScalarFieldValue p 101325
    volVectorFieldValue U (10 0 0)
);

regions
(
    zoneToCell
    {
        zone nozzleZone;
        
        fieldValues
        (
            volScalarFieldValue T 300
            volScalarFieldValue p 101325
            volVectorFieldValue U (50 0 0)
        );
    }
);


